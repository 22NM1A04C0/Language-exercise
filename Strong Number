num = int(input("Enter the value of number"))
sum = 0
Temp = num 
while (Temp!=0):
    r = Temp%10
    f=1
    i=1
    while(i<=r):
        f=f*i
        i+=1
    sum=sum+f
    Temp = Temp//10
if(sum==num):
    print("given number is Strong number")
else:
    print("given number is not a Strong number")
